<!DOCTYPE html>
<html lang="en">
  <%- include('partials/_header') %>
<body>
    <%- include('partials/_form') %>
  <h2>that's a hell of a table</h2>
  <%# console.log("the events: ", events) %>
  <%# console.log("the Books: ", sortedBooks) %>
  <table>
    <tr>
      <th>Game</th>
      <th>---</th>
      <% for (book in sortedBooks) { %>
        <th>
          <%= sortedBooks[book] %>
        </th>
      <% } %>
    </tr>
    <% for (event of events) { %>
      <tr>
        <td>Visitor</td>
        <td>
          <%= event.teams[0].name %>
        </td>
        <% for (let x in event.lines) { %>
          <td>
            HEY
            <%= event.lines[x].moneyline.moneyline_away %>
          </td>
        <% } %>
      </tr>
      <tr>
        <td>Home</td>
        <td>
          <%= event.teams[1].name %>
        </td>
        <% for (let x in event.lines) { %>
          <%= console.log("looking at one lines object: ", event.lines[x]) %>
          <td>
            <%= event.lines[x].moneyline.moneyline_home %>
          </td>
        <% } %>
      </tr>
    <% } %>
  </table>
</body>
</html>